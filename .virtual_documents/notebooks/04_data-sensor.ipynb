import os
from fnmatch import fnmatch
import re


def scanroot(root, pattern="*.csv"):
    res = []
    for path, subdirs, files in os.walk(root):
        for name in files:
            if fnmatch(name, pattern):
                r = os.path.join(path, name)
                res.append(r)
    return res


